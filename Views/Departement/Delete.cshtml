@{

    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Test.ViewModels
@model Departement
<br /> <br />
<h2 class="text-center">Delete Departement</h2>
<br />
<h3>Are you sure want to delete this?</h3>
<h4>Please, move the books in this departement to another departement if this departement contains book!!!! </h4>
<br />
<partial name="_DetailsDeletePartial" model=Model></partial>
<br />
  <form asp-action="Delete" asp-controller="Departement" asp-route-id="@Model.DepartementID" method="post">
  <button type="submit" value="Delete" class="btn btn-default">Delete</button>
  </form>
<br />
<a asp-action="index" asp-controller="Departement" class="link-success">Back to list</a>
@section AdvertiseSection
{
    <partial name="_advertisesSectionPartial"></partial>
}



