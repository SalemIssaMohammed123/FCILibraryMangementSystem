@{

    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Publisher
<br /> <br />
<h2 class="text-center">Delete Publisher</h2>
<br />
<h3>Are you sure want to delete this?</h3>
<br />
<partial name="_DetailsDeletePartial" model=Model></partial>
<br />
  <form asp-action="Delete" asp-controller="Publisher" asp-route-id="@Model.PublisherID" method="post">
  <button type="submit" value="Delete" class="btn btn-default">Delete</button>
  </form>
<br />
<a asp-action="index" asp-controller="Publisher" class="btn btn-primary">Back to list</a>
@section AdvertiseSection
{
    <partial name="_advertisesSectionPartial"></partial>
}



